{
    "name": "Px sdk injector",
    "description": "Inject sdk in any website",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "/images/16_icon.png",
        "32": "/images/32_icon.png",
        "48": "/images/48_icon.png",
        "128": "/images/128_icon.png"
      }
    },
    "permissions": [
      "tabs",
      "storage",
      "scripting",
      "activeTab",
      "cookies"
    ],
    "host_permissions": [
        "*://*/*",
        "<all_urls>"
    ],
    "icons": {
      "16": "/images/16_icon.png",
      "32": "/images/32_icon.png",
      "48": "/images/48_icon.png",
      "128": "/images/128_icon.png"
    },
    "web_accessible_resources": [{
      "resources": [
        "inject-sdk.js"
      ],
      "matches": ["<all_urls>"]
    }]
  }