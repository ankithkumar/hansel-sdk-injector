{
    "name": "Px sdk injector",
    "description": "Inject sdk in any website",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage", "activeTab", "scripting"],
    "icons": {
      "16": "/images/16_icon.png",
      "32": "/images/32_icon.png",
      "48": "/images/48_icon.png",
      "128": "/images/128_icon.png"
    },
    "content_scripts": [
      {
        "matches": ["https://*/*"],
        "run_at": "document_end",
        "js": ["injector.js"]
      }
    ],
    "web_accessible_resources": [{
      "resources": [
        "*.js"
      ],
      "matches": ["<all_urls>"]
    }],
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'",
      "sandbox": "sandbox allow-scripts; script-src 'self' 'https://*.hansel.io/*'; object-src 'self'"
    }
  }